{
	"version": 3,
	"routes": [
		{
			"handle": "filesystem"
		},
		{
			"src": "^/_astro/(.*)$",
			"headers": {
				"cache-control": "public, max-age=31536000, immutable"
			},
			"continue": true
		},
		{
			"src": "^/_server-islands/([^/]+?)/?$",
			"dest": "_render"
		},
		{
			"src": "^/_image/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/known-aliases/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/login/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/logout/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/mqtt-credentials/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/proxy(?:/(.*?))?/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/refresh/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/user/?$",
			"dest": "_render"
		}
	]
}